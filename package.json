{
  "name": "multi-replace",
  "icon": "icon.png",
  "displayName": "multi-replace",
  "description": "Replaces text and filenames in the selected catalog preserving cases.",
  "version": "0.6.0",
  "publisher": "justerest",
  "keywords": [
    "replace",
    "rename",
    "files",
    "filename",
    "angular",
    "component"
  ],
  "engines": {
    "vscode": "^1.57.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justerest/multi-replace-vsix.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.multiReplace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "extension.multiReplace",
          "group": "_multiReplace",
          "when": "explorerViewletVisible"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.multiReplace",
        "title": "multi-replace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest src",
    "test:watch": "jest --watch src"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "lodash": "^4.17.21",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@types/vscode": "^1.57.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}

{
  "name": "multi-replace",
  "displayName": "multi-replace",
  "description": "Replaces text and filenames in the selected catalog preserving cases.",
  "version": "0.3.1",
  "publisher": "justerest",
  "keywords": [
    "replace",
    "rename",
    "files",
    "filename",
    "angular",
    "component"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justerest/multi-replace-vsix.git"
  },
  "categories": [
    "Extension Packs"
  ],
  "activationEvents": [
    "onCommand:extension.multiReplace",
    "onCommand:extension.multiReplaceStrict"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "extension.multiReplace",
          "group": "_multiReplace",
          "when": "explorerViewletVisible"
        },
        {
          "command": "extension.multiReplaceStrict",
          "group": "_multiReplace",
          "when": "explorerViewletVisible"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.multiReplace",
        "title": "multi-replace"
      },
      {
        "command": "extension.multiReplaceStrict",
        "title": "multi-replace (strict)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@justerest/multi-replace": "^2.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.5",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333",
    "vscode": "^1.1.30"
  }
}
